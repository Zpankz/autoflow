{
  "name": "autoflow-workspace",
  "version": "1.0.0",
  "description": "AutoFlow AI - Knowledge Graph RAG Framework Workspace",
  "private": true,
  "scripts": {
    "nx": "nx",
    "build": "./scripts/nx-commands.sh build",
    "dev": "./scripts/nx-commands.sh dev",
    "test": "./scripts/nx-commands.sh test",
    "affected:build": "./scripts/nx-commands.sh affected:build",
    "affected:test": "./scripts/nx-commands.sh affected:test",
    "graph": "./scripts/nx-commands.sh graph",
    "affected:graph": "./scripts/nx-commands.sh affected:graph",
    "clear-cache": "./scripts/nx-commands.sh clear-cache",
    "cache-stats": "./scripts/nx-commands.sh cache-stats",
    "lint": "nx run-many --target=lint --all",
    "format": "nx run-many --target=format --all"
  },
  "devDependencies": {
    "nx": "21.4.0",
    "@nx/js": "21.4.0",
    "@nx/workspace": "21.4.0",
    "@nx/devkit": "21.4.0"
  }
}